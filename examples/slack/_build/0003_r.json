{
    "Outputs": {
        "ClioutputApigatewaySlackexampleapi": {
            "Value": {
                "Fn::Join": [
                    "",
                    [
                        "https://",
                        {
                            "Ref": "Slackexampleapi"
                        },
                        ".execute-api.",
                        {
                            "Ref": "AWS::Region"
                        },
                        ".amazonaws.com/",
                        {
                            "Ref": "Stage"
                        }
                    ]
                ]
            }
        },
        "ClioutputLambdaBotappPythonBot": {
            "Value": {
                "Ref": "PythonBotCurrentalias"
            }
        }
    },
    "Parameters": {
        "CodeBucket": {
            "Description": "Bucket where the code is located.",
            "Type": "String"
        },
        "PythonBotS3Version": {
            "Type": "String"
        },
        "Region": {
            "Description": "AWS Region",
            "Type": "String"
        },
        "Stage": {
            "Default": "dev",
            "Description": "Name of the Stage",
            "Type": "String"
        },
        "VersionS3Version": {
            "Type": "String"
        }
    },
    "Resources": {
        "BotappPythonBot": {
            "DependsOn": [
                "PythonBotRole"
            ],
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Ref": "CodeBucket"
                    },
                    "S3Key": "botapp_python_bot.zip",
                    "S3ObjectVersion": {
                        "Ref": "PythonBotS3Version"
                    }
                },
                "Description": "",
                "Handler": "code.handler",
                "MemorySize": 128,
                "Role": {
                    "Fn::GetAtt": [
                        "PythonBotRole",
                        "Arn"
                    ]
                },
                "Runtime": "python2.7",
                "Timeout": 3
            },
            "Type": "AWS::Lambda::Function"
        },
        "ContribLambdasVersion": {
            "DependsOn": [
                "VersionRole"
            ],
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Ref": "CodeBucket"
                    },
                    "S3Key": "contrib_lambdas_version.zip",
                    "S3ObjectVersion": {
                        "Ref": "VersionS3Version"
                    }
                },
                "Description": "Publishes new versions of Lambdas.",
                "Handler": "version.handler",
                "MemorySize": 128,
                "Role": {
                    "Fn::GetAtt": [
                        "VersionRole",
                        "Arn"
                    ]
                },
                "Runtime": "python2.7",
                "Timeout": 300
            },
            "Type": "AWS::Lambda::Function"
        },
        "PythonBotCurrentalias": {
            "DependsOn": [
                "PythonBotVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "BotappPythonBot"
                },
                "FunctionVersion": {
                    "Fn::GetAtt": [
                        "PythonBotVersion",
                        "Version"
                    ]
                },
                "Name": "current"
            },
            "Type": "AWS::Lambda::Alias"
        },
        "PythonBotRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "lambda.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                },
                                {
                                    "Action": [
                                        "logs:CreateLogGroup",
                                        "logs:CreateLogStream",
                                        "logs:PutLogEvents"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:logs:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "PythonBotLogsPolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "PythonBotVersion": {
            "DependsOn": [
                "VersionCurrentalias",
                "BotappPythonBot"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "BotappPythonBot"
                },
                "S3ObjectVersion": {
                    "Ref": "PythonBotS3Version"
                },
                "ServiceToken": {
                    "Ref": "VersionCurrentalias"
                }
            },
            "Type": "Custom::LambdaVersion"
        },
        "Slackexampleapi": {
            "Properties": {
                "Description": "My Slack Bot API",
                "Name": {
                    "Fn::Join": [
                        "-",
                        [
                            "slackexampleapi",
                            {
                                "Ref": "Stage"
                            }
                        ]
                    ]
                }
            },
            "Type": "AWS::ApiGateway::RestApi"
        },
        "SlackexampleapiDeploymentBee27F25": {
            "DependsOn": [
                "SlackexampleapiMyPythonBotWebhookPost"
            ],
            "Properties": {
                "RestApiId": {
                    "Ref": "Slackexampleapi"
                },
                "StageName": {
                    "Ref": "Stage"
                }
            },
            "Type": "AWS::ApiGateway::Deployment"
        },
        "SlackexampleapiMyPythonBotWebhookPost": {
            "Properties": {
                "AuthorizationType": "NONE",
                "HttpMethod": "POST",
                "Integration": {
                    "Credentials": {
                        "Fn::GetAtt": [
                            "SlackexampleapiRole",
                            "Arn"
                        ]
                    },
                    "IntegrationHttpMethod": "POST",
                    "IntegrationResponses": [
                        {
                            "SelectionPattern": "",
                            "StatusCode": "200"
                        }
                    ],
                    "RequestTemplates": {
                        "application/x-www-form-urlencoded": "{\"body\": $input.json(\"$\")}\n"
                    },
                    "Type": "AWS",
                    "Uri": {
                        "Fn::Join": [
                            "",
                            [
                                "arn:aws:apigateway:",
                                {
                                    "Ref": "AWS::Region"
                                },
                                ":lambda:path/2015-03-31/functions/",
                                {
                                    "Ref": "PythonBotCurrentalias"
                                },
                                "/invocations"
                            ]
                        ]
                    }
                },
                "MethodResponses": [
                    {
                        "StatusCode": "200"
                    }
                ],
                "ResourceId": {
                    "Ref": "SlackexampleapiResourceMyPythonBotWebhook"
                },
                "RestApiId": {
                    "Ref": "Slackexampleapi"
                }
            },
            "Type": "AWS::ApiGateway::Method"
        },
        "SlackexampleapiResourceMyPythonBotWebhook": {
            "Properties": {
                "ParentId": {
                    "Fn::GetAtt": [
                        "Slackexampleapi",
                        "RootResourceId"
                    ]
                },
                "PathPart": "my-python-bot-webhook",
                "RestApiId": {
                    "Ref": "Slackexampleapi"
                }
            },
            "Type": "AWS::ApiGateway::Resource"
        },
        "SlackexampleapiRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "apigateway.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "SlackexampleapiRolePolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "VersionCurrentalias": {
            "DependsOn": [
                "VersionVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ContribLambdasVersion"
                },
                "FunctionVersion": {
                    "Fn::GetAtt": [
                        "VersionVersion",
                        "Version"
                    ]
                },
                "Name": "current"
            },
            "Type": "AWS::Lambda::Alias"
        },
        "VersionRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "lambda.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                },
                                {
                                    "Action": [
                                        "logs:CreateLogGroup",
                                        "logs:CreateLogStream",
                                        "logs:PutLogEvents"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:logs:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "VersionLogsPolicy"
                    },
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:PublishVersion",
                                        "lambda:GetFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:lambda:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "VersionManageVersionPolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "VersionVersion": {
            "DependsOn": [
                "ContribLambdasVersion",
                "ContribLambdasVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ContribLambdasVersion"
                },
                "S3ObjectVersion": {
                    "Ref": "VersionS3Version"
                },
                "ServiceToken": {
                    "Fn::GetAtt": [
                        "ContribLambdasVersion",
                        "Arn"
                    ]
                }
            },
            "Type": "Custom::LambdaVersion"
        }
    }
}