{
    "Outputs": {
        "ClioutputLambdaExampleappS3Consumer": {
            "Value": {
                "Ref": "S3ConsumerCurrentalias"
            }
        }
    },
    "Parameters": {
        "BucketNotificationConfigurationS3Version": {
            "Type": "String"
        },
        "CodeBucket": {
            "Description": "Bucket where the code is located.",
            "Type": "String"
        },
        "Region": {
            "Description": "AWS Region",
            "Type": "String"
        },
        "S3ConsumerS3Version": {
            "Type": "String"
        },
        "SleepS3Version": {
            "Type": "String"
        },
        "Stage": {
            "Default": "dev",
            "Description": "Name of the Stage",
            "Type": "String"
        },
        "VersionS3Version": {
            "Type": "String"
        }
    },
    "Resources": {
        "BucketNotificationConfigurationCurrentalias": {
            "DependsOn": [
                "BucketNotificationConfigurationVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ContribS3BucketNotificationConfiguration"
                },
                "FunctionVersion": {
                    "Fn::GetAtt": [
                        "BucketNotificationConfigurationVersion",
                        "Version"
                    ]
                },
                "Name": "current"
            },
            "Type": "AWS::Lambda::Alias"
        },
        "BucketNotificationConfigurationRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "lambda.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                },
                                {
                                    "Action": [
                                        "logs:CreateLogGroup",
                                        "logs:CreateLogStream",
                                        "logs:PutLogEvents"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:logs:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "BucketNotificationConfigurationLogsPolicy"
                    },
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "s3:PutBucketNotification",
                                        "s3:GetBucketNotification"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:s3:::*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "BucketNotificationConfigurationManageBucketNotificationPolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "BucketNotificationConfigurationVersion": {
            "DependsOn": [
                "VersionCurrentalias",
                "ContribS3BucketNotificationConfiguration"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ContribS3BucketNotificationConfiguration"
                },
                "S3ObjectVersion": {
                    "Ref": "BucketNotificationConfigurationS3Version"
                },
                "ServiceToken": {
                    "Ref": "VersionCurrentalias"
                }
            },
            "Type": "Custom::LambdaVersion"
        },
        "ContribHelpersSleep": {
            "DependsOn": [
                "SleepRole"
            ],
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Ref": "CodeBucket"
                    },
                    "S3Key": "contrib_helpers_sleep.zip",
                    "S3ObjectVersion": {
                        "Ref": "SleepS3Version"
                    }
                },
                "Description": "Sleeps several seconds before succeeding.",
                "Handler": "sleep.handler",
                "MemorySize": 128,
                "Role": {
                    "Fn::GetAtt": [
                        "SleepRole",
                        "Arn"
                    ]
                },
                "Runtime": "python2.7",
                "Timeout": 300
            },
            "Type": "AWS::Lambda::Function"
        },
        "ContribLambdasVersion": {
            "DependsOn": [
                "VersionRole"
            ],
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Ref": "CodeBucket"
                    },
                    "S3Key": "contrib_lambdas_version.zip",
                    "S3ObjectVersion": {
                        "Ref": "VersionS3Version"
                    }
                },
                "Description": "Publishes new versions of Lambdas.",
                "Handler": "version.handler",
                "MemorySize": 128,
                "Role": {
                    "Fn::GetAtt": [
                        "VersionRole",
                        "Arn"
                    ]
                },
                "Runtime": "python2.7",
                "Timeout": 300
            },
            "Type": "AWS::Lambda::Function"
        },
        "ContribS3BucketNotificationConfiguration": {
            "DependsOn": [
                "BucketNotificationConfigurationRole"
            ],
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Ref": "CodeBucket"
                    },
                    "S3Key": "contrib_s3_bucket_notification_configuration.zip",
                    "S3ObjectVersion": {
                        "Ref": "BucketNotificationConfigurationS3Version"
                    }
                },
                "Description": "Manages Bucket Notification Configurations",
                "Handler": "bucket_notification_configuration.handler",
                "MemorySize": 128,
                "Role": {
                    "Fn::GetAtt": [
                        "BucketNotificationConfigurationRole",
                        "Arn"
                    ]
                },
                "Runtime": "python2.7",
                "Timeout": 300
            },
            "Type": "AWS::Lambda::Function"
        },
        "ExampleS3Integration": {
            "DependsOn": [
                "BucketNotificationConfigurationCurrentalias"
            ],
            "Properties": {
                "Bucket": "us-east-1-gordon-test",
                "LambdaFunctionConfigurations": [
                    {
                        "DestinationArn": {
                            "Ref": "S3ConsumerCurrentalias"
                        },
                        "Events": [
                            "s3:ObjectCreated:*",
                            "s3:ObjectRemoved:*"
                        ],
                        "Id": {
                            "Fn::Join": [
                                "-",
                                [
                                    "gordon",
                                    "3consumer_on_create_and_delete"
                                ]
                            ]
                        },
                        "KeyFilters": [
                            {
                                "Name": "suffix",
                                "Value": ".png"
                            }
                        ]
                    }
                ],
                "ServiceToken": {
                    "Ref": "BucketNotificationConfigurationCurrentalias"
                }
            },
            "Type": "Custom::S3BucketNotificationConfiguration"
        },
        "ExampleS3Integration3ConsumerOnCreateAndDeletePermission": {
            "Properties": {
                "Action": "lambda:InvokeFunction",
                "FunctionName": {
                    "Ref": "S3ConsumerCurrentalias"
                },
                "Principal": "s3.amazonaws.com",
                "SourceAccount": {
                    "Ref": "AWS::AccountId"
                }
            },
            "Type": "AWS::Lambda::Permission"
        },
        "ExampleappS3Consumer": {
            "DependsOn": [
                "S3ConsumerRole"
            ],
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Ref": "CodeBucket"
                    },
                    "S3Key": "exampleapp_s3consumer.zip",
                    "S3ObjectVersion": {
                        "Ref": "S3ConsumerS3Version"
                    }
                },
                "Description": "",
                "Handler": "code.handler",
                "MemorySize": 128,
                "Role": {
                    "Fn::GetAtt": [
                        "S3ConsumerRole",
                        "Arn"
                    ]
                },
                "Runtime": "python2.7",
                "Timeout": 3
            },
            "Type": "AWS::Lambda::Function"
        },
        "S3ConsumerCurrentalias": {
            "DependsOn": [
                "S3ConsumerVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ExampleappS3Consumer"
                },
                "FunctionVersion": {
                    "Fn::GetAtt": [
                        "S3ConsumerVersion",
                        "Version"
                    ]
                },
                "Name": "current"
            },
            "Type": "AWS::Lambda::Alias"
        },
        "S3ConsumerRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "lambda.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                },
                                {
                                    "Action": [
                                        "logs:CreateLogGroup",
                                        "logs:CreateLogStream",
                                        "logs:PutLogEvents"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:logs:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "S3ConsumerLogsPolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "S3ConsumerVersion": {
            "DependsOn": [
                "VersionCurrentalias",
                "ExampleappS3Consumer"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ExampleappS3Consumer"
                },
                "S3ObjectVersion": {
                    "Ref": "S3ConsumerS3Version"
                },
                "ServiceToken": {
                    "Ref": "VersionCurrentalias"
                }
            },
            "Type": "Custom::LambdaVersion"
        },
        "SleepCurrentalias": {
            "DependsOn": [
                "SleepVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ContribHelpersSleep"
                },
                "FunctionVersion": {
                    "Fn::GetAtt": [
                        "SleepVersion",
                        "Version"
                    ]
                },
                "Name": "current"
            },
            "Type": "AWS::Lambda::Alias"
        },
        "SleepRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "lambda.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                },
                                {
                                    "Action": [
                                        "logs:CreateLogGroup",
                                        "logs:CreateLogStream",
                                        "logs:PutLogEvents"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:logs:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "SleepLogsPolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "SleepVersion": {
            "DependsOn": [
                "VersionCurrentalias",
                "ContribHelpersSleep"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ContribHelpersSleep"
                },
                "S3ObjectVersion": {
                    "Ref": "SleepS3Version"
                },
                "ServiceToken": {
                    "Ref": "VersionCurrentalias"
                }
            },
            "Type": "Custom::LambdaVersion"
        },
        "VersionCurrentalias": {
