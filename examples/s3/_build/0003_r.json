                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "VersionVersion": {
            "DependsOn": [
                "ContribLambdasVersion",
                "ContribLambdasVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ContribLambdasVersion"
                },
                "S3ObjectVersion": {
                    "Ref": "VersionS3Version"
                },
                "ServiceToken": {
                    "Fn::GetAtt": [
