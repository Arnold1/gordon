{
    "Parameters": {
        "CodeBucket": {
            "Description": "Bucket where the code is located.",
            "Type": "String"
        },
        "JavascriptWebhookS3Version": {
            "Type": "String"
        },
        "PythonWebhookS3Version": {
            "Type": "String"
        },
        "Region": {
            "Description": "AWS Region",
            "Type": "String"
        },
        "Stage": {
            "Default": "dev",
            "Description": "Name of the Stage",
            "Type": "String"
        },
        "VersionS3Version": {
            "Type": "String"
        }
    },
    "Resources": {
        "BotappJavascriptWebhook": {
            "DependsOn": [
                "JavascriptWebhookRole"
            ],
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Ref": "CodeBucket"
                    },
                    "S3Key": "botapp_javascript_webhook.zip",
                    "S3ObjectVersion": {
                        "Ref": "JavascriptWebhookS3Version"
                    }
                },
                "Description": "",
                "Handler": "code.handler",
                "MemorySize": 128,
                "Role": {
                    "Fn::GetAtt": [
                        "JavascriptWebhookRole",
                        "Arn"
                    ]
                },
                "Runtime": "nodejs4.3",
                "Timeout": 3
            },
            "Type": "AWS::Lambda::Function"
        },
        "BotappPythonWebhook": {
            "DependsOn": [
                "PythonWebhookRole"
            ],
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Ref": "CodeBucket"
                    },
                    "S3Key": "botapp_python_webhook.zip",
                    "S3ObjectVersion": {
                        "Ref": "PythonWebhookS3Version"
                    }
                },
                "Description": "",
                "Handler": "code.handler",
                "MemorySize": 128,
                "Role": {
                    "Fn::GetAtt": [
                        "PythonWebhookRole",
                        "Arn"
                    ]
                },
                "Runtime": "python2.7",
                "Timeout": 3
            },
            "Type": "AWS::Lambda::Function"
        },
        "ContribLambdasVersion": {
            "DependsOn": [
                "VersionRole"
            ],
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Ref": "CodeBucket"
                    },
                    "S3Key": "contrib_lambdas_version.zip",
                    "S3ObjectVersion": {
                        "Ref": "VersionS3Version"
                    }
                },
                "Description": "Publishes new versions of Lambdas.",
                "Handler": "version.handler",
                "MemorySize": 128,
                "Role": {
                    "Fn::GetAtt": [
                        "VersionRole",
                        "Arn"
                    ]
                },
                "Runtime": "python2.7",
                "Timeout": 300
            },
            "Type": "AWS::Lambda::Function"
        },
        "JavascriptWebhookCurrentalias": {
            "DependsOn": [
                "JavascriptWebhookVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "BotappJavascriptWebhook"
                },
                "FunctionVersion": {
                    "Fn::GetAtt": [
                        "JavascriptWebhookVersion",
                        "Version"
                    ]
                },
                "Name": "current"
            },
            "Type": "AWS::Lambda::Alias"
        },
        "JavascriptWebhookRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "lambda.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                },
                                {
                                    "Action": [
                                        "logs:CreateLogGroup",
                                        "logs:CreateLogStream",
                                        "logs:PutLogEvents"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:logs:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "JavascriptWebhookLogsPolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "JavascriptWebhookVersion": {
            "DependsOn": [
                "VersionCurrentalias",
                "BotappJavascriptWebhook"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "BotappJavascriptWebhook"
                },
                "S3ObjectVersion": {
                    "Ref": "JavascriptWebhookS3Version"
                },
                "ServiceToken": {
                    "Ref": "VersionCurrentalias"
                }
            },
            "Type": "Custom::LambdaVersion"
        },
        "PythonWebhookCurrentalias": {
            "DependsOn": [
                "PythonWebhookVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "BotappPythonWebhook"
                },
                "FunctionVersion": {
                    "Fn::GetAtt": [
                        "PythonWebhookVersion",
                        "Version"
                    ]
                },
                "Name": "current"
            },
            "Type": "AWS::Lambda::Alias"
        },
        "PythonWebhookRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "lambda.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                },
                                {
                                    "Action": [
                                        "logs:CreateLogGroup",
                                        "logs:CreateLogStream",
                                        "logs:PutLogEvents"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:logs:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "PythonWebhookLogsPolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "PythonWebhookVersion": {
            "DependsOn": [
                "VersionCurrentalias",
                "BotappPythonWebhook"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "BotappPythonWebhook"
                },
                "S3ObjectVersion": {
                    "Ref": "PythonWebhookS3Version"
                },
                "ServiceToken": {
                    "Ref": "VersionCurrentalias"
                }
            },
            "Type": "Custom::LambdaVersion"
        },
        "Telegramexampleapi": {
            "Properties": {
                "Description": "My telegram API",
                "Name": "telegramexampleapi"
            },
            "Type": "AWS::ApiGateway::RestApi"
        },
        "TelegramexampleapiDeploymentD8565Ad8": {
            "DependsOn": [
                "TelegramexampleapiMyJsBotWebhookPost",
                "TelegramexampleapiMyPythonBotWebhookPost"
            ],
            "Properties": {
                "RestApiId": {
                    "Ref": "Telegramexampleapi"
                },
                "StageName": "dev"
            },
            "Type": "AWS::ApiGateway::Deployment"
        },
        "TelegramexampleapiMyJsBotWebhookPost": {
            "Properties": {
                "AuthorizationType": "NONE",
                "HttpMethod": "POST",
                "Integration": {
                    "Credentials": {
                        "Fn::GetAtt": [
                            "TelegramexampleapiRole",
                            "Arn"
                        ]
                    },
                    "IntegrationHttpMethod": "POST",
                    "IntegrationResponses": [
                        {
                            "SelectionPattern": "",
                            "StatusCode": "200"
                        }
                    ],
                    "Type": "AWS",
                    "Uri": {
                        "Fn::Join": [
                            "",
                            [
                                "arn:aws:apigateway:",
                                {
                                    "Ref": "AWS::Region"
                                },
                                ":lambda:path/2015-03-31/functions/",
                                {
                                    "Ref": "JavascriptWebhookCurrentalias"
                                },
                                "/invocations?Qualifier=current"
                            ]
                        ]
                    }
                },
                "MethodResponses": [
                    {
                        "StatusCode": "200"
                    }
                ],
                "ResourceId": {
                    "Ref": "TelegramexampleapiResourceMyJsBotWebhook"
                },
                "RestApiId": {
                    "Ref": "Telegramexampleapi"
                }
            },
            "Type": "AWS::ApiGateway::Method"
        },
        "TelegramexampleapiMyPythonBotWebhookPost": {
            "Properties": {
                "AuthorizationType": "NONE",
                "HttpMethod": "POST",
                "Integration": {
                    "Credentials": {
                        "Fn::GetAtt": [
                            "TelegramexampleapiRole",
                            "Arn"
                        ]
                    },
                    "IntegrationHttpMethod": "POST",
                    "IntegrationResponses": [
                        {
                            "SelectionPattern": "",
                            "StatusCode": "200"
                        }
                    ],
                    "Type": "AWS",
                    "Uri": {
                        "Fn::Join": [
                            "",
                            [
                                "arn:aws:apigateway:",
                                {
                                    "Ref": "AWS::Region"
                                },
                                ":lambda:path/2015-03-31/functions/",
                                {
                                    "Ref": "PythonWebhookCurrentalias"
                                },
                                "/invocations?Qualifier=current"
                            ]
                        ]
                    }
                },
                "MethodResponses": [
                    {
                        "StatusCode": "200"
                    }
                ],
                "ResourceId": {
                    "Ref": "TelegramexampleapiResourceMyPythonBotWebhook"
                },
                "RestApiId": {
                    "Ref": "Telegramexampleapi"
                }
            },
            "Type": "AWS::ApiGateway::Method"
        },
        "TelegramexampleapiResourceMyJsBotWebhook": {
            "Properties": {
                "ParentId": {
                    "Fn::GetAtt": [
                        "Telegramexampleapi",
                        "RootResourceId"
                    ]
                },
                "PathPart": "my-js-bot-webhook",
                "RestApiId": {
                    "Ref": "Telegramexampleapi"
                }
            },
            "Type": "AWS::ApiGateway::Resource"
        },
        "TelegramexampleapiResourceMyPythonBotWebhook": {
            "Properties": {
                "ParentId": {
                    "Fn::GetAtt": [
                        "Telegramexampleapi",
                        "RootResourceId"
                    ]
                },
                "PathPart": "my-python-bot-webhook",
                "RestApiId": {
                    "Ref": "Telegramexampleapi"
                }
            },
            "Type": "AWS::ApiGateway::Resource"
        },
        "TelegramexampleapiRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "apigateway.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "TelegramexampleapiRolePolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "VersionCurrentalias": {
            "DependsOn": [
                "VersionVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ContribLambdasVersion"
                },
                "FunctionVersion": {
                    "Fn::GetAtt": [
                        "VersionVersion",
                        "Version"
                    ]
                },
                "Name": "current"
            },
            "Type": "AWS::Lambda::Alias"
        },
        "VersionRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "lambda.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:InvokeFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": [
                                        "*"
                                    ]
                                },
                                {
                                    "Action": [
                                        "logs:CreateLogGroup",
                                        "logs:CreateLogStream",
                                        "logs:PutLogEvents"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:logs:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "VersionLogsPolicy"
                    },
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "lambda:PublishVersion",
                                        "lambda:GetFunction"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "arn:aws:lambda:*:*:*"
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "VersionManageVersionPolicy"
                    }
                ]
            },
            "Type": "AWS::IAM::Role"
        },
        "VersionVersion": {
            "DependsOn": [
                "ContribLambdasVersion",
                "ContribLambdasVersion"
            ],
            "Properties": {
                "FunctionName": {
                    "Ref": "ContribLambdasVersion"
                },
                "S3ObjectVersion": {
                    "Ref": "VersionS3Version"
                },
                "ServiceToken": {
                    "Fn::GetAtt": [
                        "ContribLambdasVersion",
                        "Arn"
                    ]
                }
            },
